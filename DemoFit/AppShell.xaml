<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DemoFit.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DemoFit.Pages"
    Shell.FlyoutBehavior="Disabled">



	
	
	<ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate local:Login}"
        Route="Login" />

	<ShellContent
        Title="AdminMenu"
        ContentTemplate="{DataTemplate local:AdminMenu}"
        Route="AdminMenu" />


	<ShellContent
        Title="ListUser"
        ContentTemplate="{DataTemplate local:ListUser}"
        Route="ListUser" />

	<ShellContent
        Title="ListProduct"
        ContentTemplate="{DataTemplate local:ListProduct}"
        Route="ListProduct" />

	<ShellContent
        Title="ProductStatics"
        ContentTemplate="{DataTemplate local:ProductStatics}"
        Route="ProductStatics" />




</Shell>
